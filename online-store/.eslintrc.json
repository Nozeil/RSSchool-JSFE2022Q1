{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": "error",
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ],
          "no-debugger": "off",
          "no-console": 0,
          "class-methods-use-this": "off"
    }
}
